<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Loarang.ViewModels"
                    xmlns:view="clr-namespace:Loarang.Views">
  
  <DataTemplate DataType="{x:Type vm:HomeVM}">
    <view:HomeView/>
  </DataTemplate>
  
  <DataTemplate DataType="{x:Type vm:ContentsVM}">
    <view:ContentsView/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type vm:ContentsSettingVM}">
    <view:ContentsSettingView/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type vm:CharAddVM}">
    <view:CharAddView/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type vm:BattleInfoVM}">
    <view:BattleInfoView/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type vm:StatsVM}">
    <view:StatsView/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type vm:SkillTreeVM}">
    <view:SkillTreeView/>
  </DataTemplate>
  <DataTemplate DataType="{x:Type vm:EquipmentVM}">
    <view:EquipmentView/>
  </DataTemplate>
</ResourceDictionary>